{
  "name": "calculus-ai-assistant",
  "displayName": "AI微积分助教",
  "version": "1.0.0",
  "description": "基于AI的微积分作业智能批改助手",
  "author": "AI微积分助教团队",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "build:firefox": "plasmo build --target=firefox-mv2",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/messaging": "^0.6.2",
    "@plasmohq/storage": "^1.9.3",
    "@supabase/supabase-js": "^2.39.0",
    "plasmo": "0.84.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "^0.34.3"
  },
  "devDependencies": {
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "http://localhost:3000/*",
      "https://yzetwnpvqioonmlibaqt.supabase.co/*"
    ],
    "permissions": [
      "storage",
      "activeTab",
      "sidePanel",
      "tabs",
      "identity"
    ],
    "background": {
      "service_worker": "/background/index.ts"
    },
    "content_security_policy": {
      "extension_pages": "script-src 'self' http://localhost:3000; object-src 'self'; connect-src 'self' http://localhost:* https://yzetwnpvqioonmlibaqt.supabase.co https://api.github.com;"
    }
  },
  "plasmo": {
    "icon": "assets/icon.png"
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad",
  "pnpm": {
    "overrides": {
      "sharp": "^0.34.3"
    }
  }
}
